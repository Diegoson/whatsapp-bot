
{
  "name": "whatsapp-bot",
  "description": "",
  "repository": "https://github.com/naxordeve/whatsapp-bot",
  "keywords": ["nodejs", "whatsapp"],
  "success_url": "/",
  "env": {
    "PREFIX": {
      "description": "Prefix for bot commands",
      "value": "/"
    },
    "OWNER_NUMBER": {
      "description": "WhatsApp number of the bot owner",
      "value": "27686881509"
    },
    "BOT_NAME": {
      "description": "Name of your bot",
      "value": "X-Astral"
    },
    "MONGODB_URI": {
      "description": "MongoDB connection string",
      "required": false
    },
    "SESSION_ID": {
      "description": "WhatsApp session ID",
      "required": true
    },
    "PASTE_BIN_API": {
      "description": "Your paste key for session id",
      "required": true
    },
    "GROK_API": {
      "description": "Groq sdk key for ai",
      "required": true
    },
    "WORK_TYPE": {
      "description": "Bot work type (private/public)",
      "value": "private"
    }
  }
}
